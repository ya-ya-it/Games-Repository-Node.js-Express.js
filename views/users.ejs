<% include partials/header %>

  <div class="row">
    <div class="col-10 col">
      <h1>
        <%= title %>
      </h1>
    </div>
  </div>

  <br>
  <table>
    <thead>
      <tr>
        <th>User ID</th>
        <th>Username</th>
      </tr>
    </thead>
    <tbody>

      <% for (let i = 0; i < users.length; i++) { %>

        <tr>
          <td>
            <%= users[i]._id %>
          </td>
          <td>
            <%= users[i].username %>
          </td>
        </tr>

        <% } %>
    </tbody>
  </table>
  <% include partials/footer %>
